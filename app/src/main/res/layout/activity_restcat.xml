<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradientback"
    tools:context=".Restcat">


    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_row="0"
        android:layout_column="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>


    <TextView
        android:id="@+id/textres"
        android:layout_width="410dp"
        android:layout_height="67dp"
        android:gravity="center"
        android:text="Restaurants Categories"
        android:textColor="@android:color/black"
        android:background="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/searchView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </TextView>

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchView2"
        android:layout_width="407dp"
        android:layout_height="61dp"
        android:layout_marginTop="10dp"
        android:minWidth="?attr/actionBarSize"
        android:background="@drawable/searchview_bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097">

    </androidx.appcompat.widget.SearchView>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="410dp"
        android:layout_height="518dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView2">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:alignmentMode="alignMargins"
            android:columnCount="3"
            android:columnOrderPreserved="false"
            android:padding="14dp"
            android:rowCount="3">

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton1"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/ameran"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="American Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>


            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton2"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/andhra"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Andhra Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton3"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/arabic"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Arabic Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton4"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/awa1"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Awadhi Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton5"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/barbeque"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Barbeque Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton6"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/bengali"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Bengali Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton7"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/continental"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Continental Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton8"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/coastal"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Coastal Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton9"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/punjabires"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Punjabi Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>


            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton10"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/europeanres"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="European Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/frenchres"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="French Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>

            <LinearLayout android:orientation="vertical">

                <ImageButton
                    android:id="@+id/imageButton12"
                    android:layout_width="100dp"
                    android:layout_height="90dp"
                    android:layout_margin="10dp"
                    android:src="@drawable/maha"
                    tools:ignore="SpeakableTextPresentCheck"/>

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="110dp"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:text="Maharashtrian Restaurants"
                    android:textColor="@color/black"
                    android:textSize="16dp">

                </TextView>
            </LinearLayout>


        </GridLayout>
    </ScrollView>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        android:background="#ff1717"
        app:menu="@menu/nav_items" />
</androidx.constraintlayout.widget.ConstraintLayout>